{
  "name": "blockchain-finality-benchmark",
  "version": "2.0.0",
  "description": "Multi-chain finality benchmarking tool for NEAR, Ethereum, Arbitrum, Solana, Base, Optimism, Polygon, Avalanche, BSC, zkSync, Polygon zkEVM, Aptos, and Sui",
  "main": "dist/benchmark.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/benchmark.js",
    "test": "npm run build && node dist/benchmark.js --network all --txs 5",
    "near": "npm run build && node dist/benchmark.js --network near --txs 10",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "blockchain",
    "finality",
    "benchmark",
    "near",
    "ethereum",
    "arbitrum",
    "solana",
    "base",
    "optimism",
    "polygon",
    "avalanche",
    "bsc",
    "zksync",
    "aptos",
    "sui"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.32.0",
    "@mysten/sui.js": "^0.54.1",
    "@solana/web3.js": "^1.87.6",
    "axios": "^1.8.4",
    "bs58": "^5.0.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "got": "^11.8.6",
    "near-api-js": "7.0.0-rc.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
